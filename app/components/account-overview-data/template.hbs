<div
  {{did-insert this.getData}}
  {{did-update this.getData @accountId}}
>
  {{#if this.dataInstance.isRunning}}
    <AccountsValueLoading/>
  {{/if}}
  {{#if this.dataInstance.isSuccessful}}
    <AccountOverview
            @account={{this.dataInstance.value.account}}
            @chartData={{this.dataInstance.value.chartData}}
    />
  {{/if}}
</div>